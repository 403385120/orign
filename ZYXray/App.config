  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
  </startup>
  <userSettings>
    <ZYXray.Properties.Settings>
      <setting name="Language" serializeAs="String">
        <value>en-us</value>
      </setting>
    </ZYXray.Properties.Settings>
  </userSettings>
