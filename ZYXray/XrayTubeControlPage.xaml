                        VerticalAlignment="Center"
                        Content="{Binding XRayTube2Status.ActualCurrent}"
                        ContentStringFormat="{}{0} uA"
                        FontSize="10" />
                </Border>

                <Border
                    Grid.Row="1"
                    Grid.Column="7"
                    Background="{Binding XRayTube2Status.IsInterLockOn, Converter={StaticResource BooleanToBrushConverter}}"
                    CornerRadius="5">
                    <Label
