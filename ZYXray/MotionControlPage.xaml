                <RowDefinition Height=".6*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width=".13*" />
                <ColumnDefinition Width=".25*" />
                <ColumnDefinition Width=".13*" />
                <ColumnDefinition Width=".13*" />
            </Grid.ColumnDefinitions>
            <Grid Grid.Row="0" Grid.Column="2" Margin="3">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="0.5*" />
                    <ColumnDefinition Width="2*" />
                    <ColumnDefinition Width="4*" />
                    <ColumnDefinition Width="0.7*" />
                </Grid.ColumnDefinitions>
                <Label Content="工艺路线:" FontSize="13" VerticalAlignment="Center" Grid.Row="0" Grid.Column="1" />
                <ComboBox SelectedIndex="{Binding ReworkMode, UpdateSourceTrigger=PropertyChanged}" x:Name="cbo_ReworkMode" FontSize="13" Grid.Row="0" Grid.Column="2" IsEnabled="False">
                    <ComboBoxItem Content="OB - XRay - FQI"/>
                    <ComboBoxItem Content="OC - XRay - FQI"/>
                    <ComboBoxItem Content="O4 - XRay - FQI"/>
                    <ComboBoxItem Content="XRay - FQI"/>
                    <ComboBoxItem Content="O4 - FQI"/>
                    <ComboBoxItem Content="OG - FQI"/>
                    <ComboBoxItem Content="FQI"/>
                    <ComboBoxItem Content="OB-XRAY"/>
                    <ComboBoxItem Content="OCVG"/>
                    <ComboBoxItem Content="离线生产"/>
                </ComboBox>
            </Grid>

            <Grid Grid.Row="0" Grid.Column="3" Margin="5">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>

                <Button 
                    IsEnabled="{Binding MyCncButton.IsStartBtnEnable}" 
                    Grid.Row="0"
                    Grid.Column="1"
                    Command="{Binding StartTest}"
                    Content="开始" >
                </Button>
                <Button 
                    IsEnabled="{Binding MyCncButton.IsStopBtnEnable}"
                    Grid.Row="0"
                    Grid.Column="2"
                    Command="{Binding StopTest}"
                    Content="停止">
                </Button>
            </Grid>
            <Label 
                Content="{Binding AlgoInitFinHint}" 
                Grid.Column="3" Margin="6,5,128,-35" Grid.Row="3"
                />
        </Grid>

